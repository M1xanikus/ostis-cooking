        <- definition;;
        => nrel_idtf: [Определение (Филиппинский рыбный суп) ] (* <-lang_ru;; *);;
        <= nrel_sc_text_translation: ... (*
            -> rrel_example: [ Это ароматный и наваристый суп из рыбы, овощей и специй, популярный на Филиппинах.  ] (* <-lang_ru;; *);;
        *);;*);
        => nrel_recipe: ... (*
            -> rrel_ingredients: ... (*
                -> concept_fish;;
		-> concept_tomatoes;;
		-> concept_onion;;
		-> concept_ginger;;
		-> concept_garlic;;
		-> concept_chili_pepper;;
		-> concept_fish_sauce;;
		-> concept_lime_juice;;
		-> concept_water;;
		-> concept_salt;;
		-> concept_pepper;;
            *);;
            -> rrel_instruction:...(*
                => nrel_idtf: [Рецепт (Филиппинский рыбный суп)] (* <-lang_ru;; *);;
                <= nrel_sc_text_translation: ... (* -> rrel_example: "file://../../../figures/html/filipino_fish_soup.txt"(*  => nrel_format: format_txt;;  *);;  *);;
            *);;
        *);
