        concept_azrb_bozbash;
        concept_azrb_dovga;
        concept_azrb_baklava;
        concept_azrb_basturma;
        concept_azrb_petey;
        concept_azrb_kyukyu;
        concept_azrb_kutaby_with_greens;;

concept_azerbaijani_cuisine
    <-sc_node_not_relation;
    =>nrel_main_idtf:[азербайджанская кухня](*<-lang_ru;;*);
    =>nrel_main_idtf:[azerbaijani cuisine](*<-lang_en;;*);
    =>nrel_specifics: [предпочтение мясным блюдам из птицы, баранины или говядины.](* <-lang_ru;; *);
