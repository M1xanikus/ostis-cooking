    ->rrel_key_sc_element: subject_domain_of_azerbaijani_cuisine;;  


subject_domain_of_azerbaijani_cuisine
    =>nrel_main_idtf:[Предметная область азербайджанской кухни] (* <-lang_ru;;*);
    =>nrel_main_idtf:[Subject domain of azerbaijani cuisine] (* <-lang_en;;*);
